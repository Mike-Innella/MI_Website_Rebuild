{
  "name": "website-ai-server",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/server.ts",
    "start": "NODE_ENV=production tsx src/server.ts",
    "backfill:chunks": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/backfill_kb_chunks.ts",
    "test:db": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/test_db.ts",
    "test:rag": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/test_rag_chunks.ts",
    "test:rag:docs": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/test_rag_retrieval.ts",
    "test:rag:db": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/test_db_match_self.ts",
    "test:all": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/test_all.ts",
    "embed": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/embed_kb_docs.ts",
    "seed:kb": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/seed_kb_docs_from_site.ts",
    "seed:facts": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/seed_assistant_facts.ts",
    "seed:all": "DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/seed_kb_docs_from_site.ts && DOTENV_CONFIG_PATH=.env.local NODE_ENV=development tsx src/scripts/seed_assistant_facts.ts"
  },
  "dependencies": {
